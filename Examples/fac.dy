-- Self-recursive binding
fac (n: Num) =
  match n
  | 0 -> 1
  | _ -> n * fac (n - 1)
and

-- With fixed points
fac' f (n: Num) =
  match n
  | 0 -> 1
  | _ -> n * f (n - 1)
and

-- Haskell-style `fix`
fix f = let x = f x in x and
facF = fix fac' and

-- Y combinator
y f = (\x. f (x x)) (\x. f (x x)) and
facY = y fac' and

main =
  print (fac 100) *>
  print (facF 100) *>
  print (facY 100)

{
    "name": "dynasty-vscode",
    "displayName": "Dynasty",
    "description": "Dynasty Syntax Highlighting",
    "publisher": "aionescu",
    "icon": "icon.png",
    "repository": {
      "type": "git",
      "url": "https://github.com/aionescu/dynasty.git"
    },
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "dy",
            "aliases": ["Dynasty", "dy"],
            "extensions": [".dy"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "dy",
            "scopeName": "source.dy",
            "path": "./syntaxes/dy.tmLanguage.json"
        }]
    }
}

module List

import Core.Function
import Core.IO
import Core.List
import Core.Math

-- Mutual recursion
a = 1 :: b and
b = 2 :: a and

main =
  do   print $ 1 .. 15
  then print $ 15 .. 1
  then print $ -5 .. 5
  then print $ map ((+) 2) $ 1 .. 10
  then print $ take 20 $ fix \a -> 1 :: a
  then print $ take 20 a
